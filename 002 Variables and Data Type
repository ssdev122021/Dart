void main(){
   int age = 23;
   double height = 5.4;
   String name = 'Shine Shine';
   bool isSingle = true;
   var lst = [1,2,2,0,true,'Shine Shine'];
   var post = {'title' : 'post one','desc' : 'post one','like' :100, 'publish' : true};
   var heart_symbol = '\u2665';  
   var laugh_symbol = '\u{1f600}';  

   print(name);
   print(age);
   print(height);
   print(isSingle);
   print(lst);
   print(post);
   print(heart_symbol);  
   print(laugh_symbol);  
}
